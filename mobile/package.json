{
  "name": "prisbo-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:android": "expo prebuild --platform android",
    "prebuild:clean": "expo prebuild --clean",
    "build:android": "cd android && ./gradlew assembleDebug && cd ..",
    "build:android:release": "cd android && ./gradlew assembleRelease && cd ..",
    "build:apk": "bash build-apk-local.sh",
    "build:apk:release": "bash build-apk-local.sh --release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "axios": "^1.6.2",
    "expo": "~54.0.0",
    "expo-build-properties": "~1.0.10",
    "expo-image-picker": "~17.0.0",
    "expo-status-bar": "~3.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.11.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.0.2",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/metro-config": "^0.83.1"
  },
  "private": true
}
